<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.idam.passwordmanagement.model.user.UserDetails" table="CLG_DIARY_USER_DETAILS">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="org.hibernate.id.MultipleHiLoPerTableGenerator">
				<param name="table">AFBMSEQUENCE</param>
				<param name="primary_key_value">CLG_DIRY_USER_ID</param>
				<param name="value_column">SEQUENCE_VALUE</param>
				<param name="primary_key_column">SEQUENCE_NAME</param>
				<param name="max_lo">0</param>
			</generator>
		</id>

		<one-to-one name="masterUser" class="com.idam.passwordmanagement.model.user.MasterUser" fetch="join" lazy="proxy">
        </one-to-one>

		<property name="firstName" type="java.lang.String">
			<column name="FIRSTNAME" not-null="true" />
		</property>

		<property name="lastName" type="java.lang.String">
			<column name="LASTNAME" />
		</property>

		<property name="dateofbirth" type="java.util.Date">
			<column name="DATE_OF_BIRTH" />
		</property>

		<property name="sex" type="java.lang.String">
			<column name="SEX" />
		</property>

		<property name="userType" type="java.lang.String">
			<column name="USER_TYPE" />
		</property>

	</class>
</hibernate-mapping>
